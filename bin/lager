import sys
import os

if os.environ.get('LC_CTYPE', '') == 'UTF-8':
    os.environ['LC_CTYPE'] = 'en_US.UTF-8'

sys.path.append(os.path.dirname(os.path.dirname(os.path.realpath(__file__))))

from lager.cli import cli

if __name__ == '__main__':
    sys.exit(cli())
